{
  "title": "Easy Passwords",
  "name": "easypasswords",
  "id": "easypasswords@palant.de",
  "version": "1.0.2",
  "description": "Use a single master password to generate secure unique passwords for all websites - without losing track of the passwords you cannot change.",
  "main": "lib/main.js",
  "author": "Wladimir Palant",
  "homepage": "https://github.com/palant/easypasswords",
  "permissions": {
    "private-browsing": true,
    "multiprocess": true
  },
  "preferences": [
    {
      "name": "autolock",
      "type": "bool",
      "value": true,
      "title": " "
    },
    {
      "name": "autolock_delay",
      "type": "integer",
      "value": 10,
      "title": " "
    }
  ],
  "engines": {
    "firefox": "41.0 - 49.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "globals": {
      "console": false
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off",
      "semi": ["error", "always"]
    }
  },
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palant/easypasswords.git"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-htmlhint": "^0.3.1",
    "gulp-json-modify": "^1.0.0",
    "gulp-less": "^3.0.5",
    "gulp-rename": "^1.2.2",
    "gulp-zip": "^3.2.0",
    "jpm": "^1.0.6",
    "merge-stream": "^1.0.0",
    "node-rsa": "^0.3.2",
    "vinyl-source-stream": "^1.1.0"
  }
}
